{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "algolia/algoliasearch-client-php": "^3.1",
        "barryvdh/laravel-snappy": "^1.0",
        "cyrildewit/eloquent-viewable": "^6.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "http-interop/http-factory-guzzle": "^1.2",
        "jantinnerezo/livewire-alert": "^2.2",
        "jorenvanhocht/laravel-share": "^4.2",
        "laravel/framework": "^8.54",
        "laravel/jetstream": "^2.4",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.7",
        "laravel/tinker": "^2.5",
        "laravolt/avatar": "^4.1",
        "livewire/livewire": "^2.5",
        "maize-tech/laravel-markable": "^1.0",
        "mckenziearts/laravel-notify": "^2.2",
        "meilisearch/meilisearch-php": "^0.26.1",
        "protonemedia/laravel-form-components": "^3.6",
        "rappasoft/laravel-livewire-tables": "^2.8",
        "spatie/laravel-permission": "^5.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
